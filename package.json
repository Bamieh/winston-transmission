{
  "name": "winston-transmission",
  "version": "0.0.1",
  "description": "Winston Transmission: nodeJS logger built on top of winston",
  "main": "./distribution/index.js",
  "author": "Ahmad Bamieh",
  "license": "MIT",
  "dependencies": {
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-inject": "^0.0.4",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-node5": "^12.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-spies": "^0.7.1",
    "coveralls": "^2.13.1",
    "cross-env": "^4.0.0",
    "jsdom": "^9.12.0",
    "mkdirp": "^0.5.1",
    "mocha": "^3.3.0",
    "nyc": "^10.2.0",
    "rimraf": "^2.6.1",
    "webpack": "^3.0.0",
    "webpack-node-externals": "^1.6.0"
  },
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha",
    "test:watch": "yarn run test -- --reporter min --watch",
    "test:cover": "cross-env NODE_ENV=test nyc yarn run test",
    "build": "cross-env NODE_ENV=production babel src --out-dir distribution --compact=true --no-comments",
    "prepublish": "yarn run build",
    "coverage:html": "yarn run test:cover && nyc report --reporter=html",
    "coverage:lcov": "yarn run test:cover && nyc report --reporter=lcov",
    "coverage": "yarn run coverage:html && open coverage/index.html",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "codecov": "codecov"
  }
}
